            <th>
                @Html.DisplayNameFor(model => model.CbNgaysinh)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CbCmnd)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CoquanCqMaNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.KhenthuongcbKtcbMaNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.KyluatcbKlcbMaNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CbHoten)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CbGioitinh)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CbDiachi)
