            <td>
                @Html.DisplayFor(modelItem => item.KhenthuongcbKtcbMaNavigation.KtcbMa)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.KyluatcbKlcbMaNavigation.KlcbMa)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CbMa">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CbMa">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CbMa">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
